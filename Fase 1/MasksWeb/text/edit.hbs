<input type="text"
name="{{name}}"
value="{{value}}"
{{#if def.len}}maxlength="{{def.len}}"{{/if}}
{{#if def.placeholder}}placeholder="{{str def.placeholder this.model.module}}"{{/if}}
class="inherit-width"
{{#eq name 'cnpj_c'}}
onkeyup="mascara(this,mascara_cnpj);"
{{/eq}}
{{#eq name 'billing_address_postalcode'}}
onkeyup="mascara(this,mascara_cep);"
{{/eq}}
{{#eq name 'shipping_address_postalcode'}}
onkeyup="mascara(this,mascara_cep);"
{{/eq}}
{{#eq name 'primary_address_postalcode'}}
onkeyup="mascara(this,mascara_cep);"
{{/eq}}
{{#eq name 'alt_address_postalcode'}}
onkeyup="mascara(this, mascara_cep);"
{{/eq}}
{{#eq name 'first_name'}}
onkeyup="mascara(this, mascara_name_contatos);"
{{/eq}}
{{#eq name 'last_name'}}
onkeyup="mascara(this, mascara_name_contatos);"
{{/eq}}
{{#eq action 'disabled'}} disabled{{/eq}}>
<p class="help-block">
<script type="text/javascript" src="custom/masks/mask.js"></script>

