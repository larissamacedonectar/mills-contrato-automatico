<input type="phone"
name="{{name}}"
value="{{value}}"
{{#if def.len}}maxlength="{{def.len}}"{{/if}}
{{#if def.placeholder}}placeholder="{{str def.placeholder this.model.module}}"{{/if}}
class="inherit-width"
{{#eq name 'phone_home'}}
onkeypress="mascara(this,mascara_fone);"
{{/eq}}
{{#eq name 'phone_mobile'}}
onkeypress="mascara(this,mascara_fone);"
{{/eq}}
{{#eq name 'phone_work'}}
onkeypress="mascara(this,mascara_fone);"
{{/eq}}
{{#eq name 'phone_fax'}}
onkeypress="mascara(this,mascara_fone);"
{{/eq}}
{{#eq name 'phone_other'}}
onkeypress="mascara(this,mascara_fone);"
{{/eq}}
{{#eq name 'phone_office'}}
onkeypress="mascara(this,mascara_fone);"
{{/eq}}
{{#eq name 'phone_alternate'}}
onkeypress="mascara(this,mascara_fone);"
{{/eq}}
{{#eq action 'disabled'}} disabled{{/eq}}>
<p class="help-block">
<script type="text/javascript" src="custom/mask/mask.js"></script>

