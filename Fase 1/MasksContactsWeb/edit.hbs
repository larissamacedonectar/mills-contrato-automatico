<input type="text"
name="{{name}}"
value="{{value}}"
{{#if def.len}}maxlength="{{def.len}}"{{/if}}
{{#if def.placeholder}}placeholder="{{str def.placeholder this.model.module}}"{{/if}}
class="inherit-width"
{{#eq name 'phone_c'}}
onkeyup="mascara(this,mascara_fone);"
{{/eq}}
{{#eq name 'phone_work'}}
onkeyup="mascara(this,mascara_fone);"
{{/eq}}
{{#eq name 'cnpj_c'}}
onkeyup="mascara(this,mascara_cnpj);"
{{/eq}}
{{#eq name 'cep_c'}}
onkeyup="mascara(this,mascara_cep);"
{{/eq}}
{{#eq action 'disabled'}} disabled{{/eq}}>
<p class="help-block">
<script type="text/javascript" src="custom/valmer/mask.js"></script>

